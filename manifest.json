{
    "manifest_version": 2,
    "name": "ani!Search",
    "version": "1.1",
  
    "description": "Displays info on the anime that an osu! beatmap is from",
  
    "icons": {
      "96": "icons/icon-96.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://osu.ppy.sh/*"],
        "js": ["ani!Search.js"],
        "run_at": "document_end"
      }
    ],

    "permissions": [
      "webRequest",
      "https://graphql.anilist.co/",
      "https://api.chimu.moe/v1/*"
    ]
}
  